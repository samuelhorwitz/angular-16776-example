    "memory-cache": "^0.1.6",
    "nodemon": "^1.11.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "string-replace-loader": "^1.0.5",
    "ts-node": "3.0.2",
    "typed-graphql": "^1.0.2",
    "typescript": "^2.2",
    "v8-lazy-parse-webpack-plugin": "^0.3.0",
    "webpack": "2.1.0-beta.27",
