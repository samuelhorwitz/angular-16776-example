{
  "name": "angular-16776-example",
  "version": "0.0.1",
  "description": "angular-16776-example",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samuelhorwitz/angular-16776-example.git"
  },
  "engines": {
    "node": "7.3.0"
  },
  "engineStrict": true,
  "private": true,
  "author": "Samuel Julius Horwitz",
  "license": "Public Domain",
  "scripts": {
    "start:prod": "node dist/server/index.js",
    "watch": "webpack --progress --colors --watch -d",
    "watch:dev": "yarn run server & yarn run watch",
    "clean:dist": "yarn run rimraf dist",
    "clean:ngc": "yarn run rimraf **/*.ngfactory.ts **/*.css.shim.ts",
    "prebuild": "yarn run clean:dist",
    "build": "webpack --progress --colors -d",
    "build:prod:ngc": "yarn run clean:ngc && yarn run ngc && yarn run clean:dist && yarn run build:prod",
    "build:prod:ngc:json": "yarn run clean:ngc && yarn run ngc && yarn run clean:dist && yarn run build:prod:json",
    "build:prod": "webpack --config webpack.prod.config.ts --progress --colors -p",
    "build:prod:json": "webpack --progress --colors -p --config webpack.prod.config.ts --json | webpack-bundle-size-analyzer",
    "ngc": "ngc -p tsconfig.aot.json",
    "prestart": "yarn run build",
    "server": "nodemon -L dist/server/index.js",
    "debug:server": "node-nightly --inspect --debug-brk dist/server/index.js",
    "start": "yarn run server",
    "debug:start": "yarn run build && yarn run debug:server",
    "predebug": "yarn run build",
    "debug:build": "node-nightly --inspect --debug-brk node_modules/webpack/bin/webpack.js",
    "debug:build:prod": "node-nightly --inspect --debug-brk node_modules/webpack/bin/webpack.js  --config webpack.prod.config.ts",
    "debug": "node --debug-brk dist/server/index.js"
  },
  "dependencies": {
    "@angular/animations": "^4.1.2",
    "@angular/common": "^4.1.2",
    "@angular/compiler": "^4.1.2",
    "@angular/compiler-cli": "^4.1.2",
    "@angular/core": "^4.1.2",
    "@angular/forms": "^4.1.2",
    "@angular/http": "^4.1.2",
    "@angular/platform-browser": "^4.1.2",
    "@angular/platform-browser-dynamic": "^4.1.2",
    "@angular/platform-server": "^4.1.2",
    "@angular/router": "^4.1.2",
    "@types/bluebird": "^3.0.36",
    "@types/chai": "^3.4.34",
    "apollo-angular": "^0.13.0",
    "apollo-client": "^1.2.2",
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "buffer": "^5.0.2",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.5.6",
    "express": "^4.14.0",
    "graphql-tag": "^1.3.1",
    "isomorphic-fetch": "^2.2.1",
    "js.clone": "0.0.3",
    "lodash": "^4.17.3",
    "methods": "^1.1.2",
    "morgan": "^1.7.0",
    "preboot": "^4.5.2",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.2.0",
    "text-encoding": "^0.6.0",
    "tslib": "^1.6.0",
    "webfontloader": "^1.6.26",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.0",
    "@ngtools/webpack": "~1.3.0",
    "@types/body-parser": "0.0.29",
    "@types/compression": "0.0.29",
    "@types/cookie-parser": "^1.3.29",
    "@types/express": "^4.0.32",
    "@types/express-serve-static-core": "^4.0.33",
    "@types/hammerjs": "^2.0.32",
    "@types/memory-cache": "0.0.29",
    "@types/mime": "0.0.28",
    "@types/morgan": "^1.7.32",
    "@types/node": "^6.0.38",
    "@types/serve-static": "^1.7.27",
    "@types/webfontloader": "^1.6.27",
    "accepts": "^1.3.3",
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "^3.1.2",
    "cookie-parser": "^1.4.3",
    "express-interceptor": "^1.2.0",
